<h1 class = "title-left-black"><%= @homecook.user.name  %></h1>
<div class = "title-middle" ><%= rand(40..50)/10.0 %><i class="fas fa-star big-star"></i></div>

  <div class= "image-block">
    <% if @homecook.photos.attached? %>

    <% @homecook.photos.each do |photo| %>
    <%= cl_image_tag photo.key, height:200, width:250, crop: :fill, :class => "style_image" %>
    <% end %>
    <% else %>
    <img src="https://source.unsplash.com/featured/?meals&<%= rand(10000) %>" alt="" width="picture" height="300", width="400" border="0" />
  <% end %>

</div>


  <div class = "title-middle-bold"> About Me </div>
  <div> <%= @homecook.introduction %> </div>






